{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "framebook.authapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/auth/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "auth"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "framebook.profissionalapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/profissional/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "profissional"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "framebook.stackapi",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/stack/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "stack"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "Auth API",
          "Version": "v1",
          "Url": " http://framebook.authapi:80/swagger/v1/swagger.json"
        }
      ]
    },

    {
      "Key": "profissional",
      "Config": [
        {
          "Name": "Profissional API",
          "Version": "v1",
          "Url": "http://framebook.profissionalapi:80/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "stack",
      "Config": [
        {
          "Name": "Stack API",
          "Version": "v1",
          "Url": "http://framebook.stackapi:80/swagger/v1/swagger.json"
        }
      ]
    }
  ],

  "GlobalConfiguration": {
    "BaseUrl": "http://localhost"
  }
}